{
  "name": "actions-netlify",
  "version": "1.2.3",
  "private": true,
  "description": "GitHub Actions for Netlify",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build lib/main.js",
    "esbuild": "esbuild src/main.ts --bundle --outdir=dist --platform=node",
    "test": "jest",
    "all": "yarn run build && yarn run format && yarn run lint && yarn run package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwtgck/actions-netlify.git"
  },
  "keywords": [
    "actions",
    "Netlify"
  ],
  "author": "Ryo Ota <nwtgck@nwtgck.org> (https://github.com/nwtgck)",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.9.0",
    "@actions/github": "5.0.3",
    "netlify": "6.1.29"
  },
  "devDependencies": {
    "esbuild": "0.14.48",
    "@types/jest": "28.1.3",
    "@types/node": "16.0.0",
    "@typescript-eslint/parser": "5.30.0",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.18.0",
    "eslint-plugin-github": "4.3.6",
    "eslint-plugin-jest": "26.5.3",
    "jest": "28.1.2",
    "jest-circus": "28.1.2",
    "js-yaml": "4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4"
  }
}