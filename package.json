{
  "name": "actions-netlify",
  "version": "2.0.0",
  "private": true,
  "description": "GitHub Actions for Netlify",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwtgck/actions-netlify.git"
  },
  "keywords": [
    "actions",
    "Netlify"
  ],
  "author": "Ryo Ota <nwtgck@nwtgck.org> (https://github.com/nwtgck)",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "netlify": "^6.1.29"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^16.18.6",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "jest-circus": "^29.3.1",
    "jest-mock": "^29.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.3"
  }
}
