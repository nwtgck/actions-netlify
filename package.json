{
  "name": "actions-netlify",
  "version": "1.0.4",
  "description": "GitHub Actions for Netlify",
  "keywords": [
    "actions",
    "Netlify"
  ],
  "homepage": "",
  "bugs": "",
  "license": "MIT",
  "author": "Ryo Ota <nwtgck@nwtgck.org> (https://github.com/nwtgck)",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwtgck/actions-netlify.git"
  },
  "scripts": {
    "build": "ncc build src/main.ts",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run lint:tsc && npm run build"
  },
  "dependencies": {
    "@actions/core": "^1.2.3",
    "@actions/github": "^2.1.1",
    "netlify": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^12.12.29",
    "@typescript-eslint/parser": "^2.22.0",
    "@zeit/ncc": "^0.21.1",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^23.8.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "private": true
}
